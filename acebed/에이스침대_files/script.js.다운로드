                    }, function(){
                        if(_device == "pc"){
                            $(this).removeClass("hover");
                        }
                    });

                    $(".mat_find_area .mat_step .select_list > a").eq(index).on("click", function(){
                        if(!$(this).hasClass("on")){
                            $(this).parent(".select_list").find("a").removeClass("on");
