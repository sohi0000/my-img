                            if(self.progress >= 1){
                                $(".ace_his_area .lineP2 .year_btn a").eq(2).addClass("on");
                            }else{
                                $(".ace_his_area .lineP2 .year_btn a").eq(2).removeClass("on");
                            }
                        },
                    },
                    scaleX:0,
