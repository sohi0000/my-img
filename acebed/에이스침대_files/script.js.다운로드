        scrollTriggerEvt : function(){
            gsap.registerPlugin(ScrollTrigger);
            ScrollTrigger.defaults({
                //markers:true,
            });
            
            // 제품 - 스트레스리스
            if($(".stressless_con4").length != 0){
                var stresslessMotion1 = gsap.timeline();
                    stresslessMotion1.fromTo(".stressless_con4 .six_list li:nth-child(1)", { opacity: 0 }, { opacity: 1 }, 0.25)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(1) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.3)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(2)", { opacity: 0 }, { opacity: 1 }, 0.3)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(2) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.35)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(3)", { opacity: 0 }, { opacity: 1 }, 0.35)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(3) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.4)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(4)", { opacity: 0 }, { opacity: 1 }, 0.4)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(4) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.45)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(5)", { opacity: 0 }, { opacity: 1 }, 0.5)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(5) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.55)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(6)", { opacity: 0 }, { opacity: 1 }, 0.55)
                                    .fromTo(".stressless_con4 .six_list li:nth-child(6) .before span", { 'transform': "translate(0, 100px)", opacity: 0 }, { 'transform': "translate(0, 0px)", opacity: 1 }, 0.6)

